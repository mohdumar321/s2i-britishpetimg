fastcgi_param  ROSI_SESSION        $cookie_phpsessid;
fastcgi_param  ROSI_CODEROOT      /opt/app-root/src/appl/rosi/rosi-sol;
fastcgi_param  ROSI_CODEWWWROOT    /opt/app-root/src/appl/rosi/rosi-sol/wwwroot;
